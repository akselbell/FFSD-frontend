{
"compilerOptions": {
    "target": "es2015",
    "lib": [
        "DOM",
        "DOM.Iterable",
        "ESNext"
    ],
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": true,
    "noEmit": false,
    "jsx": "react"
},
"include": [
    "src"
],
"exclude": [
    "node_modules"
],
}